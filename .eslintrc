{
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    },
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "globals": {
        "$": false,
        "describe": false,
        "it": false
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "block-scoped-var": 0,
        "lass-methods-use-this": 0,
        "comma-dangle": 0,
        "class-methods-use-this": 0,
        "func-names": 0,
        "indent": [2, 4],
        "jsx-a11y/no-static-element-interactions": 0,
        "max-len": [2, 120],
        "no-tabs": 0,
        "react/jsx-indent": 0,
        "react/forbid-prop-types": 0,
        "react/jsx-filename-extension": 0,
        "react/no-unused-prop-types": 0
    },
    "plugins": [
        "react"
    ]
}
