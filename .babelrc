{
  "presets": [
    [
      "@babel/preset-env",
      {
        "modules": false,
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": [
    "transform-class-properties",
    "transform-es2015-spread"
  ],
  "env": {
    "test": {
      "presets": ["@babel/preset-env", "@babel/preset-react"],
      "plugins": [
        "transform-class-properties",
        "transform-es2015-spread",
        "@babel/plugin-transform-runtime",
        "transform-react-jsx"
      ]
    }
  }
}
